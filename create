oc delete project ci
echo "sleeping for 60 secs"
sleep 60
oc new-project ci --display-name="CI/CD"
oc policy add-role-to-user edit -z default -n ci
oc create -f jenkins-slave-builder-template.yaml
oc create -f jenkins-master-s2i-template.yaml
echo "sleeping for 60 secs"
sleep 60
oc new-app jenkins-slave-build
oc new-app jenkins-master-s2i
